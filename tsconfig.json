{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "outDir": "./lib",
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@App/*":["src/*"],
      "lib/*":["common/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/__tests__/*"],
}
